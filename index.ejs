<html>
<style>
	table, th, td {
  		border: 1px solid black;
	}
</style>
	<body style='background-color: rgb(235,235,235)'>
		<form action="/create_id" method="GET">
			<label for="id">id:</label><br>
			<input type="text" name="new_id"/><br>
			<label for="pw">Password:</label><br>
			<input type="password" name="new_password"/><br>
			<button>Create</button>
		</form>
		This will show a list of pur posts
		<% result.forEach(function(item){%>
			<table>
	  			<tr>
	    				<th><%= item.user_id %></th>
	 			</tr>
	  			<tr>
	    				<td><%= item.password %></td>
	
	  			</tr>
	  			<tr>
	    			<form action="/delete_id/:item.user_id" method="GET">
					<br><button name="delete_id" value="1234">Delete</button> //may need to change
					</form>
	  			</tr>
			</table>
		<% }); %>	
	</body>
</html>